{
  "name": "patientregistration",
  "version": "0.1.0",
  "description": "OWA for offline patient registration",
  "repository": {
    "type": "git",
    "url": "https://github.com/ivange94/openmrs-owa-patientregistration"
  },
  "engines": {
    "npm": "^4.0.5",
    "node": "^7.4.0"
  },
  "eslintConfig": {
    "plugins": [
      "angular"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "dependencies": {
    "angular": "^1.5.5",
    "angular-animate": "^1.5.5",
    "angular-resource": "^1.5.11",
    "angular-route": "^1.5.11",
    "angular-translate": "^2.15.2",
    "angular-ui-router": "^0.3.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.3",
    "archiver": "^1.0.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "browser-sync": "^2.11.1",
    "browser-sync-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^4.0.0",
    "eslint-plugin-angular": "^2.4.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.24.1",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "ng-annotate-webpack-plugin": "0.1.3",
    "on-build-webpack": "^0.1.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "sw-precache": "^5.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "yargs": "^4.3.1"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf coverage*",
    "build": "npm run clean && webpack --progress --colors --mode=production --target=web",
    "build:dev": "npm run clean && webpack --progress --colors --mode=dev --target=web && npm run sw",
    "build:prod": "npm run build",
    "build:deploy": "webpack --progress --colors --mode=deploy --target=web",
    "watch": "webpack --progress --colors --watch --mode=deploy --target=web",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js"
  },
  "keywords": [
    "OpenMRS",
    "Open",
    "Web",
    "App"
  ],
  "author": "ivange94",
  "license": "MPL-2.0"
}
